<div class="name">
{% set cast = value['value']|replace({ 'Actors:': '' }) %}
{% for cast in cast|split(',') %}
<a href="/cast/{{ slug(cast|lower|spaceless) }}">{{ cast }}</a>,
{% endfor %}
</div>
</div>
</div></div>
                       
                         {% endif %}
                        {% endfor %}


