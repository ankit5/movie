<div class="" id="load">
</div>
</div>
{% if node.field_episodes.0.value %} 
            <div id="seasons">
            <div class="tvseason">
            <div class="les-title"> <i class="fa fa-server mr5"></i>
            <strong>{{ node.field_season_title.0.value }}</strong>
            </div>
            <div class="les-content" style="display: block">
            {% for key,value in node.field_episodes.getValue %}
             <a href="javascript:void(0)" data-value="{{ node.id }}" data-key="{{ key }}" data-types="eps"> Episode {{ key+1 }} </a> 
             {% set key = key+1 %}
             {% endfor %}
             </div>
             </div>
             </div>
{% endif %}
{% if node.field_download_url.1.value %} 
            <div id="seasons">
            <div class="tvseason">
            <div class="les-content" style="display: block;padding: 0;">
            {% set i=1 %} 
            {% for key,value in node.field_player.getValue %}
             {% if ('.xyz' in value['value']) or ('minoplres' in value['value']) %}
             {% set pl_text = (node.field_player_text.getValue[key]['value'])?node.field_player_text.getValue[key]['value']:"File " ~ i %}
             {% set selected = (i==1)?'selected':'' %}
             <a href="javascript:void(0)" data-value="{{ node.id }}" data-key="{{ key }}" data-types="eps" class="{{ selected }}"> {{ pl_text }} </a> 
             {% set i=i+1 %} 
             {% endif %}
             {% set key = key+1 %}
             {% endfor %}
             </div>
             </div>
             </div>
{% endif %}
<div class="dooplay_player" style="display:none">
<div id="playeroptions" class="options pmovie__big-btns d-flex jc-center">
 <div class="player_nav">
    <ul id="playeroptionsul pmovie__stream-select" class="ajax_mode tabs-block__select idTabs">
    {% for key,value in node.field_player.getValue %}
      {% if ('.xyz' in value['value']) or ('minoplres' in value['value']) %}
         {% set class_tab = '' %}
          {% else %}
         {% set class_tab = 'tab_load' %}
          {% endif %}
    <li id="player-option-{{ key }}" class="dooplay_player_option nopl" data-value="{{ node.id }}" data-key="{{ key }}" class="{{ class_tab }}" style="display: block;"><i class="fas fa-play-circle"></i><span class="title">Server {{ key }}</span><span class="loader"></span></li>
   
    {% endfor %}
    </ul>
    
    </div>

</div>
</div>
<div class="sheader">
<div class="data">
<h1>{{ node.label }}</h1>
<a href="/contact/feedback"
  class="use-ajax"
  data-dialog-type="modal"
  data-dialog-options="{&quot;dialogClass&quot;:&quot;foobar&quot;}">Report</a>
<div class="extra">
<span class="date" itemprop="dateCreated">
{% for key,value in node.field_right.getValue %}
                        {% if 'Release:' in value['value'] %}
                        
                         {% set year = value['value']|replace({ 'Release:': '' }) %}
                         <a href="/release-year/{{ year|spaceless }}">{{ year }}</a>
                         {% endif %}
                   {% endfor %}
</span>
<span class="country">
{% for key,value in node.field_left.getValue %}
                        {% if 'Country:' in value['value'] %}
                        
                         {% set country = value['value']|replace({ 'Country:': '' }) %}
                          {% for country in country|split(',') %}
                         <a href="/country/{{ slug(country|lower|spaceless) }}">{{ country }}</a>,
                          {% endfor %}

                         {% endif %}
                        {% endfor %}
</span>
<span itemprop="duration" class="runtime"> 
{% if node.field_episodes.0.value is null %}
                   {% for key,value in node.field_right.getValue %}
                        {% if 'Duration:' in value['value'] %}
                        
                         {{ value['value']|replace({ 'Duration:': '' }) }}
                        
                         {% endif %}
                   {% endfor %}
                  {% endif %}
</span>
</div>
<div class="starstruck-ptype" style="">
<div>
<meta itemprop="name" content="{{ node.label }}">
<div itemscope="" class="starstruck-wrap" >
<div class="dt_rating_data">
<div class="starstruck-rating">
  {% for key,value in node.field_right.getValue %}
                        {% if 'IMDb:' in value['value'] %}
                        <span class="dt_rating_vgs">{{ value['value']|replace({ 'IMDb:': '' }) }}</span><i class="fas fa-user-circle"></i>
<span class="rating-text">IMDb</span>
                         {% endif %}
                   {% endfor %}

                    {% for key,value in node.field_right.getValue %}
