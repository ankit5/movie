<div class="item">
    <label {{ title_attributes.addClass(title_classes) }}>{{ label }} : </label>
    {% if multiple %}
      <div>
    {% endif %}
    {% for item in items %}
      <div class="value">{{ twig_json_decode(item.content|render|striptags|trim|lower,'embed_url')|render }}</div>
    {% endfor %}
    {% if multiple %}
      </div>
    {% endif %}
  </div>
